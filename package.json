{
  "name": "catalyst",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=22.20.0",
    "bun": ">=1.3.0"
  },
  "scripts": {
    "dev": "next dev",
    "dev-email": "email dev --port 3001",
    "analyze": "ANALYZE=true next build --webpack",
    "build": "next build",
    "build:standalone": "EXPORT_MODE=standalone next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "lint-staged": "lint-staged",
    "format": "biome format",
    "format-write": "biome format --write",
    "lint": "biome lint",
    "lint-write": "biome lint --write",
    "check": "biome check",
    "check-write": "biome check --write",
    "update:patch": "ncu -u --target patch -p bun && bun i",
    "update:minor": "ncu -u --target minor -p bun && bun i",
    "configure": "bun scripts/configure.ts",
    "prepare": "husky",
    "postinstall": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "bun run check"
    ]
  },
